{
  "env": {
    "development": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": "current"
            }
          }
        ]
      ],
      "plugins": [
        "add-module-exports"
      ]
    },
    "production": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": "current"
            }
          }
        ], "minify"
      ],
      "plugins": [
        "add-module-exports"
      ]
    }
  }
}
